{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Organisation",
  "type": "object",
  "properties": {
    "address": {
      "$ref": "./Address.schema.json"
    },
    "freeText": {
      "type": "boolean"
    },
    "id": {
      "type": "string"
    },
    "locations": {
      "type": "array",
      "items": {
        "$ref": "./Location.schema.json"
      }
    },
    "name": {
      "type": "string"
    },
    "organisationType": {
      "type": "string"
    },
    "ssoEnabled": {
      "type": "boolean"
    },
    "edossierEnabled": {
      "type": "boolean"
    },
    "edossierProviderId": {
      "type": "string"
    },
    "edossierProviderName": {
      "type": "string"
    },
    "domainNames": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}