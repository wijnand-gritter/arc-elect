{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Product",
  "type": "object",
  "properties": {
    "attributeId": {
      "type": "integer"
    },
    "attributes": {
      "type": "array",
      "items": {
        "$ref": "./ProductAttributes.schema.json"
      }
    },
    "category": {
      "type": "string"
    },
    "categoryIds": {
      "type": "array",
      "items": {
        "$ref": "./ProductCategoryIds.schema.json"
      }
    },
    "cons": {
      "type": "string"
    },
    "currency": {
      "$ref": "../common/enums/CurrencyEnum.schema.json"
    },
    "dateCreated": {
      "type": "string"
    },
    "dateUpdated": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "ean": {
      "type": "string"
    },
    "externalId": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "internalId": {
      "type": "string"
    },
    "invoiceLocation": {
      "$ref": "./Location.schema.json"
    },
    "medium": {
      "type": "array",
      "items": {
        "$ref": "./ProductMedium.schema.json"
      }
    },
    "name": {
      "type": "string"
    },
    "options": {
      "type": "array",
      "items": {
        "$ref": "./ProductOptions.schema.json"
      }
    },
    "price": {
      "type": "number"
    },
    "pros": {
      "type": "string"
    },
    "referenceId": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "specification": {
      "type": "string"
    },
    "status": {
      "type": "integer"
    },
    "subCategory": {
      "type": "string"
    },
    "subCategoryNumber": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "typeId": {
      "type": "string"
    },
    "visibility": {
      "type": "string"
    },
    "volume": {
      "type": "number"
    },
    "volumeUnit": {
      "type": "string"
    },
    "weight": {
      "type": "number"
    },
    "weightUnit": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
