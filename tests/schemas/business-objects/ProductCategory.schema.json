{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ProductCategory",
  "type": "object",
  "properties": {
    "active": {
      "type": "boolean"
    },
    "categorieId": {
      "type": "integer"
    },
    "code": {
      "type": "string"
    },
    "level": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "parentId": {
      "type": "integer"
    },
    "position": {
      "type": "integer"
    },
    "productCategories": {
      "type": "array",
      "items": {
        "$ref": "./ProductCategory.schema.json"
      }
    },
    "productCount": {
      "type": "integer"
    },
    "products": {
      "type": "array",
      "items": {
        "$ref": "./Product.schema.json"
      }
    }
  },
  "additionalProperties": false
}